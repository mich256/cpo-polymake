script:

load('alcoved.sage')

#pi = DecoratedPermutation([3,1,6,5,2,4],[0]*6)
pi = DecoratedPermutation([1,3,2],[0]*3)
G = DP_to_GN(pi)
BDP = GN_to_bdp(G)
R = RootSystem(['A', 2])
Q = AlcovedPolytope(R, BDP)
Lambdacheck = R.coweight_lattice().fundamental_weights()
search(Q)
P = Positroid(G)
trueP = P.projected_polytope

load('hstar.sage')
trueP.h_star_vector()